{
  "name": "beaker-dashboard",
  "version": "1.0.0",
  "description": "Beaker Dashboard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beaker-project/beaker-dashboard.git"
  },
  "author": "Beaker Developers <developers@beaker-project.org>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/beaker-project/beaker-dashboard/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "prettier": "prettier --check \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run prettier && npm run lint && npm run test"
    }
  },
  "homepage": "https://github.com/beaker-project/beaker-dashboard#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "@types/jest": "^26.0.0",
    "@types/webpack": "^4.41.17",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.0.7",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.18.5",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}

    "@types/react-router-dom": "^5.3.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.31.2",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
